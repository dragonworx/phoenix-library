{
  "parser": "babel-eslint",
  "extends": "plugin:react/recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
     "ecmaVersion": 6,
     "sourceType": "module",
     "ecmaFeatures": {
       "jsx": true
     }
  },
  "globals": {
    "window": true,
    "document": true,
    "console": true,
    "process": true,
    "Promise": true
  },
  "rules": {
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "compat/compat": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "func-names": 0,
    "generator-star-spacing": [0],
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/no-unresolved": 2,
    "new-cap": 0,
    "no-implicit-coercion": 0,
    "no-mixed-operators": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "no-var": "error",
    "no-unused-vars": 2,
    "no-undef": 2,
    "semi": ["error", "always"],
    "promise/param-names": 2,
    "promise/always-return": 0,
    "promise/catch-or-return": 0,
    "promise/no-native": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  },
  "plugins": [
    "compat",
    "import",
    "promise"
  ]
}
