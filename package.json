{
  "name": "react-babel-webpack-boilerplate",
  "version": "1.4.1",
  "description": "A boilerplate for React-Babel-Webpack project",
  "main": "client/main.jsx",
  "scripts": {
    "lint": "eslint --cache --ignore-path .gitignore --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "build": "webpack",
    "build-watch": "webpack --watch",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "server-prod": "node ./server",
    "server-dev": "nodemon ./server"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-runtime": "6.x",
    "body-parser": "^1.18.2",
    "cli-color": "^1.2.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "material-ui": "^1.0.0-beta.37",
    "morgan": "^1.9.0",
    "passport-custom": "^1.0.5",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "15.x",
    "react-dom": "15.x",
    "react-router-dom": "^4.2.2",
    "sequelize": "^4.35.2",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "7.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "~4.5.0",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.0",
    "eslint-plugin-import": "~2.9.0",
    "eslint-plugin-jsx-a11y": "3.x",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "~7.7.0",
    "husky": "^0.12.0",
    "nodemon": "^1.17.1",
    "open-browser-webpack-plugin": "0.0.3",
    "style-loader": "~0.13.1",
    "validate-commit-msg": "2.x",
    "webpack": "^2.0.10",
    "webpack-cli": "^2.0.10"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ali Chamas",
  "license": "commercial",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
