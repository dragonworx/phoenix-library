{
  "name": "phoenix_lib",
  "version": "1.3.0",
  "description": "For Liz",
  "main": "client/main.jsx",
  "scripts": {
    "build-dev": "PHOENIX_LIB_ENV=dev webpack --watch",
    "build-local": "PHOENIX_LIB_ENV=dev webpack",
    "build-prod": "webpack",
    "server-dev": "nodemon ./server",
    "server-prod": "pm2 start ./server/index.js --name phoenix_lib",
    "hash": "node ./scripts/hash.js",
    "dbadmin": "node ./scripts/db-admin.js",
    "adduser": "node ./scripts/adduser.js",
    "test-api": "node ./scripts/api.js",
    "start": "yarn build-dev & yarn server-dev"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^1.1.0",
    "@devexpress/dx-react-grid": "^1.1.0",
    "@devexpress/dx-react-grid-material-ui": "^1.1.0",
    "aws-sdk": "^2.213.1",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "6.x",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^0.1.19",
    "cli-color": "^1.2.0",
    "draft-js": "^0.10.5",
    "draft-js-export-html": "^1.2.0",
    "draft-js-import-html": "^1.2.1",
    "ejs": "^2.5.7",
    "eslint": "^4.18.2",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.15.6",
    "fscreen": "^1.0.2",
    "happn-password-hash-and-salt": "^1.1.0",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "morgan": "^1.9.0",
    "passport-custom": "^1.0.5",
    "pdfkit": "^0.8.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "16.3.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "sequelize": "^4.35.2",
    "serve-favicon": "^2.4.5",
    "sharp": "^0.20.1"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "1.x",
    "copy-webpack-plugin": "~4.5.0",
    "css-loader": "~0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "hard-source-webpack-plugin": "^0.6.4",
    "husky": "^0.12.0",
    "nodemon": "^1.17.1",
    "open-browser-webpack-plugin": "0.0.3",
    "pre-build-webpack": "^0.1.0",
    "style-loader": "~0.13.1",
    "validate-commit-msg": "2.x",
    "webpack": "^2.0.10",
    "webpack-build-notifier": "^0.1.23"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ali Chamas",
  "license": "commercial",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
